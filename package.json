{
  "name": "fly-babel",
  "version": "0.8.0",
  "description": "Babel plugin for Fly",
  "license": "MIT",
  "repository": "https://github.com/bucaran/fly-babel",
  "main": "index.js",
  "scripts": {
    "lint": "eslint *.js",
    "setup": "npm i && npm test",
    "spec": "npm run test | tspec",
    "test": "npm run lint && npm run test-harmony",
    "build": "echo No build task specified.",
    "deploy": "npm run test && git push origin master && npm publish",
    "test-harmony": "node --harmony `which tape` test/*.js | `which tspec`"
  },
  "author": "Jorge Bucaran",
  "dependencies": {
    "babel-core": "^6.3.0",
    "babel-preset-es2015": "^6.3.0",
    "object-assign": "^3.0.0",
    "read-pkg-up": "^1.0.1",
    "source-map": "^0.4.4"
  },
  "devDependencies": {
    "eslint": "^0.21.2",
    "tap-spec": "^4.0.2",
    "tape": "^4.0.0"
  },
  "engines": {
    "iojs": ">= 1.0.0",
    "node": ">= 0.12.0"
  },
  "keywords": [
    "fly",
    "fly-plugin",
    "babel",
    "es6",
    "compile",
    "transpile",
    "6to5",
    "harmony",
    "ES.next",
    "ecmascript"
  ]
}
